{
  "name": "notion-stuff",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "test": "nx test",
    "commit": "git-cz",
    "release": "dotenv release-it --",
    "build": "nx run-many --target=build --all --verbose",
